{
  "name": "pdf-viewer-element",
  "version": "0.1.0-pre.4",
  "description": "A web component for displaying PDFs",
  "main": "pdf-viewer.js",
  "module": "pdf-viewer.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf pdf-viewer.{d.ts,d.ts.map,js,js.map} test/pdf-viewer.{d.ts,d.ts.map,js,js.map} test/pdf-viewer_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/* --write",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/pdf-viewer.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "es-dev-server --root-dir=docs --node-resolve --watch",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "serve": "es-dev-server --node-resolve --watch  --babel-exclude=**/@bundled-es-modules/pdfjs-dist/build/*",
    "test": "karma start karma.conf.cjs",
    "test:watch": "karma start karma.conf.cjs --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start karma.conf.cjs --update-snapshots",
    "test:prune-snapshots": "karma start karma.conf.cjs --prune-snapshots",
    "checksize": "rollup -c ; cat pdf-viewer.bundled.js | gzip -9 | wc -c ; rm pdf-viewer.bundled.js"
  },
  "keywords": [
    "pdf"
  ],
  "author": "Justin Fagnani <justin@fagnani.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/justinfagnani/pdf-viewer-element.git"
  },
  "dependencies": {
    "@bundled-es-modules/pdfjs-dist": "^2.1.266",
    "@material/mwc-fab": "^0.8.0",
    "@material/mwc-icon-button": "^0.12.0",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.9.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^2.0.3",
    "@material/mwc-switch": "^0.7.1",
    "@material/mwc-textfield": "^0.7.1",
    "@material/mwc-top-app-bar-fixed": "^0.7.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@types/pdfjs-dist": "^2.1.5",
    "@types/resize-observer-browser": "^0.1.3",
    "es-dev-server": "^1.57.4",
    "lit-analyzer": "^1.2.1",
    "rollup": "^1.27.13",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.3.1",
    "web-component-analyzer": "^1.1.6"
  }
}
